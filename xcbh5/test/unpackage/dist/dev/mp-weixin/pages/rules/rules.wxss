
.me-container {
	overflow: hidden;
	width: 100%;
	height: 100vh;
	box-sizing: border-box;
	padding: 0rpx 50rpx 00rpx 50rpx;
	color: white;
	z-index: 1;
	background-color: #d1d1d1;
	display: flex;
	flex-direction: column;
	/* justify-content: center; */
	color: black;
}
.title {
	height: 100rpx;
	text-align: center;
	line-height: 100rpx;
	font-size: 34rpx;
	/* margin-top: 30rpx; */
	margin-bottom: 30rpx;
	/* background-color: aqua; */
}
.fixd{
	height: 100rpx;
	width: 80%;
	color: white;
	font-size: 30rpx;
	position: fixed;
	bottom: 50rpx;
	left: 50%; /* 使元素相对于视口宽度居中 */
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%); /* 将元素向左移动一半的自身宽度，实现居中对齐 */
	display: flex;
	justify-content: center;
	align-items: center;
	background-color: #007aff;
	border-radius: 20rpx;
}

