<view class="me-container"><scroll-view class="Stallholder" style="{{'height:'+('100vh')+';'}}" scroll-y="true" data-event-opts="{{[['scrolltolower',[['handleScrollToLower',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{pageData}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="item-list"><view data-event-opts="{{[['tap',[['gotostaldis',['$0'],[[['pageData','id',item.id,'id']]]]]]]}}" class="logo" bindtap="__e"><image class="logos" src="{{item.logo}}" mode="aspectFill"></image></view><view data-event-opts="{{[['tap',[['gotostaldis',['$0'],[[['pageData','id',item.id,'id']]]]]]]}}" class="info" bindtap="__e"><view class="message">{{"菜市场名称："+item.market_name}}</view><view class="message">{{"摊位名称："+item.title}}</view><view class="message">{{"摊位编号："+item.id}}</view></view><view class="interface"><view data-event-opts="{{[['tap',[['editShop',['$0'],[[['pageData','id',item.id]]]]]]]}}" class="edit" bindtap="__e">编辑</view><view data-event-opts="{{[['tap',[['deleteShop',['$event']]]]]}}" class="delete" bindtap="__e">删除</view></view></view></block><block wx:if="{{pageLoading}}"><view class="loading">加载中...</view></block><block wx:if="{{!hasMore}}"><view class="loading">没有更多了</view></block></scroll-view><block wx:if="{{showEditPopup}}"><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="popup-overlay" bindtap="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="popup" catchtap="__e"><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="close-button" bindtap="__e">×</view><view class="popup-content"><view class="content"><view class="cuisine"><view class="title">摊位名称</view><view class="shuru"><input class="shurucon" type="text" data-event-opts="{{[['input',[['__set_model',['','currentShopTitle','$event',[]]]]]]}}" value="{{currentShopTitle}}" bindinput="__e"/></view></view><view class="editimg"><view class="title">摊位图片</view><image class="cimg" src="{{currentShopLogo}}" mode="aspectFit" data-event-opts="{{[['tap',[['uploadcuisine',['$event']]]]]}}" bindtap="__e"></image></view></view></view><view class="popup-footer"><button data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['confirmEdit',['$event']]]]]}}" style="background-color:#007aff;color:white;" bindtap="__e">确认</button></view></view></view></block></view>