.me-container{width:100%;box-sizing:border-box;padding:0rpx 25rpx 0 25rpx;color:#fff;z-index:1;background-color:#f8f8f8;display:flex;flex-direction:column;justify-content:center}.title{font-size:35rpx;color:#000;text-align:center;margin-top:50rpx}.records{width:100%;height:50rpx;margin-top:30rpx;color:#000;display:flex}.market{width:33%;height:100%;text-align:center}.usejifen{flex:1;height:100%;display:flex;flex-direction:column;justify-content:center;align-items:center}.time{width:33%;height:100%;text-align:center}.record-item{width:100%;color:#000;display:flex;margin-top:10rpx;margin-bottom:10rpx;justify-content:space-between;border-bottom:1px solid #ccc}.record-item .market{flex:1;height:120rpx;line-height:120rpx;text-align:center;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.record-item .time{flex:1;height:120rpx;line-height:120rpx;text-align:center;font-size:22rpx;overflow:hidden;white-space:nowrap;text-overflow:ellipsis}.record-item .usejifen{flex:1;height:120rpx;line-height:120rpx;font-size:25rpx}.judgment{height:50%;display:flex;justify-content:center;align-items:center}.green-text{color:green}.red-text{color:red}.balance{height:50%;display:flex;justify-content:center;align-items:center}.lottery{position:fixed;left:50%;bottom:50rpx;width:90%;height:120rpx;color:#fff;background-color:#007aff;-webkit-transform:translateX(-50%);transform:translateX(-50%);border-radius:20rpx;text-align:center;line-height:120rpx;font-size:35rpx}.loading{text-align:center;padding:20rpx;color:#999}