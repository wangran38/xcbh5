<view class="me-container"><view class="basicsinfo"><text style="margin:0rpx 20rpx;">基本信息</text><view class="item-info"><view class="name"><text>联系人</text><input type="text" placeholder="请填写联系人" data-event-opts="{{[['input',[['__set_model',['','contactpeople','$event',[]]]]]]}}" value="{{contactpeople}}" bindinput="__e"/></view><view class="phone"><text>联系电话</text><input type="text" placeholder="请填写联系人电话" data-event-opts="{{[['input',[['__set_model',['','contactphone','$event',[]]]]]]}}" value="{{contactphone}}" bindinput="__e"/></view></view></view><view class="store"><text style="margin:10rpx 20rpx;">摊铺信息</text><view class="storename"><text>摊铺名称</text><input type="text" placeholder="请填写摊铺名称" data-event-opts="{{[['input',[['__set_model',['','title','$event',[]]]]]]}}" value="{{title}}" bindinput="__e"/></view><view class="area"><text>所在地区</text><picker class="picker" mode="multiSelector" range="{{multiArray}}" value="{{multiIndex}}" data-event-opts="{{[['change',[['bindMultiPickerChange',['$event']]]],['columnchange',[['bindMultiPickerColumnChange',['$event']]]]]}}" bindchange="__e" bindcolumnchange="__e"><view class="picker-text">{{''+multiArray[0][multiIndex[0]]+" - "+multiArray[1][multiIndex[1]]+" -\n\t\t\t\t\t"+multiArray[2][multiIndex[2]]+''}}</view></picker></view><view class="Address"><text>选择菜市场</text><picker class="picker" mode="selector" range="{{marketList}}" value="{{selectedMarketIndex}}" data-event-opts="{{[['change',[['bindMarketChange',['$event']]]]]}}" bindchange="__e"><view class="picker-text">{{marketList[selectedMarketIndex]}}</view></picker></view><view class="Category"><text>所售类目</text><picker class="Categorypicker" mode="selector" range="{{categoryList}}" data-event-opts="{{[['change',[['bindCategoryChange',['$event']]]]]}}" bindchange="__e"><view>{{selectedCategory}}</view></picker></view><view class="stallphone"><text>摊主电话</text><input type="text" placeholder="请填写摊主电话" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="BL1"><text>摊主图片</text><button data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="BLpicker" bindtap="__e">{{isImageSelected?'已选择':'选择图片'}}</button></view><view class="illustrate"><text>摊位的详细内容说明</text><textarea placeholder-style="color:#666666" placeholder="请输入详细内容说明" data-event-opts="{{[['input',[['__set_model',['','content','$event',[]]]]]]}}" value="{{content}}" bindinput="__e"></textarea></view></view><button class="submit" type="primary" disabled="{{isSubmitting}}" data-event-opts="{{[['tap',[['submitForm',['$event']]]]]}}" bindtap="__e">提交</button></view>